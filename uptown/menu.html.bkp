<script type="text/javascript">
$().ready(function() {

	$("#slidesjs").slidesjs({
		width: 1800,
		height: 800,
		navigation: false,
		pagination: false
	});

	/*$('#slides').slides({
		preloadImage: 'img/ajax-loader.gif',
		preload: true,
		generatePagination: false,
		generateNextPrev: false,
		effect: 'fade',
		crossfade: true,
		bigTarget: true
	});*/

	$('#page-hdr').localScroll(500);
	$('.tabs a').click(function() {
		$('.menu-section, .download').hide();
		var id = $(this).attr('id')+'-menu';
		$('#'+id).fadeIn();
		$('#'+id+'-download').fadeIn();
		$(this).parent().parent().find('a').removeClass('current');
		$(this).addClass('current');
		return false;
	});
	$('.view-menu').delay(1000).fadeIn('slow');
	//do something when element is showing
	$('.nav').waypoint(function(event, direction) {
		if (direction === 'down') {
			$('.view-menu').fadeOut();
		}
		else {
			$('.view-menu').fadeIn();
		}
	});

	$(".nav").tinyNav();
});
</script>
