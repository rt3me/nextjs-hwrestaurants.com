	    },
		pagination: false,
	    play: {
	      active: true,
	      effect: "fade",
	      interval: 3000,
	      auto: true,
	    },
		effect: {
          fade: {
            speed: 800
          }
        }
	});

	/*$('#slides').slides({
		preloadImage: 'img/loader.gif',
		preload: true,
		generatePagination: false,
		generateNextPrev: true,
		effect: 'fade',
		crossfade: true,
		bigTarget: true
	});*/

	$('.down').delay(1000).fadeIn('slow');
	$('#down').localScroll(800);
	//do something when element is showing
	$('.nav').waypoint(function(event, direction) {
		if (direction === 'down') {
			$('.down').fadeOut();
		}
		else {
			$('.down').fadeIn();
		}
	});

	$(".nav").tinyNav();
});
</script>
